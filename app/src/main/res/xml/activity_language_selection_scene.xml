<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="143dp"
            android:id="@+id/japaneseButton"
            motion:layout_constraintTop_toBottomOf="@+id/hindiButton"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintVertical_bias="0.32"
            motion:layout_constraintHorizontal_bias="0.473" />
        <Constraint
            android:layout_height="57dp"
            android:layout_width="132dp"
            android:id="@+id/hindiButton"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.475"
            motion:layout_constraintVertical_bias="0.313"
            motion:layout_constraintStart_toStartOf="@+id/imageView2"
            android:layout_marginStart="45dp"
            android:layout_marginEnd="70dp"
            motion:layout_constraintTop_toTopOf="@+id/imageView" />
        <Constraint
            android:layout_height="48dp"
            android:layout_width="136dp"
            android:id="@+id/englishButton"
            motion:layout_constraintBottom_toBottomOf="@+id/imageView3"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="90dp"
            android:layout_marginEnd="45dp"
            motion:layout_constraintHorizontal_bias="0.534"
            motion:layout_constraintVertical_bias="0.0" />
        <Constraint
            android:layout_height="157dp"
            android:layout_width="419dp"
            android:id="@+id/imageView"
            motion:layout_constraintStart_toStartOf="@+id/imageView"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="484dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintVertical_bias="0.011"
            motion:layout_constraintHorizontal_bias="0.0" />
        <Constraint
            android:layout_height="182dp"
            android:layout_width="414dp"
            android:id="@+id/imageView2"
            motion:layout_constraintTop_toBottomOf="@+id/imageView"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintVertical_bias="0.0"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_constraintBottom_toTopOf="@+id/japaneseButton"
            motion:layout_editor_absoluteX="3dp" />
        <Constraint
            android:layout_height="185dp"
            android:layout_width="405dp"
            android:id="@+id/imageView3"
            motion:layout_constraintTop_toBottomOf="@+id/imageView2"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintVertical_bias="0.0"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_editor_absoluteX="6dp" />
        <Constraint
            android:layout_height="211dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="411dp"
            android:id="@+id/imageView4"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.0" />
        <Constraint
            android:layout_height="53dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="115dp"
            android:id="@+id/spanishButton"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="156dp"
            android:layout_marginRight="156dp"
            android:layout_marginBottom="80dp"
            motion:layout_editor_absoluteX="140dp" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
    </ConstraintSet>
</MotionScene>